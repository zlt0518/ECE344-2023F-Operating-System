 DEPFILE_UNQUOTED = ext2-create.p/src_ext2-create.c.o.d
 ARGS = -Iext2-create.p -I. -I.. -fcolor-diagnostics -D_FILE_OFFSET_BITS=64 -Wall -Winvalid-pch -Wextra -Wpedantic -std=c17 -O0 -g -D_DEFAULT_SOURCE

build ext2-create: c_LINKER ext2-create.p/src_ext2-create.c.o
 LINK_ARGS = -Wl,--as-needed -Wl,--no-undefined


 description = $DESC
 restat = 1

rule REGENERATE_BUILD
 command = /usr/bin/meson --internal regenerate /home/zhoulita/ece344/hello-ext2 .
 description = Regenerating build files.
 generator = 1

# Phony build target, always out of date

build PHONY: phony 

# Build rules for targets

build ext2-create.p/src_ext2-create.c.o: c_COMPILER ../src/ext2-create.c
